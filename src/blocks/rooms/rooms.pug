      +e.inner
        +e.row
          +e.visual(id="single-box")
            +e.SPAN.arrow.--prev
            +e.SPAN.arrow.--next
            +b.slider(id="single")
              +e.item
                img(src="img/rooms/single-standart/photo2.jpg", alt="Одноместный Стандарт")
              +e.item
                img(src="img/rooms/single-standart/photo4.jpg", alt="Одноместный Стандарт")
              +e.item
                img(src="img/rooms/single-standart/photo5.jpg", alt="Одноместный Стандарт")
